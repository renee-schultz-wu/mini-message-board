<html>
    <head>
        <title>Homepage</title>
    </head>
    <body>
        <a href="/new"> Add a new message </a>

        <ul>
            <% messages.forEach((message) => { %>
                <%- include('message', {message: message}); %>
                <%- console.log(message.id)  %>
                <a href="/view?<%=message.id%>"> view message  </a>
            <% }); %>
          </ul>
          
        </ul>
    </body>
</html>